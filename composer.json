  "description": "Library for validating email address with emailhippo service",
  "keywords": ["email", "validation", "emailhippo"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksym Hubar",
      "email": "maxim.gubar@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/log": "^1.1",
    "symfony/http-client": "^5.1 || ^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Nrgone\\EmailHippo\\Validator\\": "src/"
    }
